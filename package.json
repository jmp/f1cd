{
  "name": "f1cd",
  "version": "0.0.1",
  "description": "A simple countdown website for Formula One events.",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unit": "react-scripts test --coverage --watchAll=false",
    "test:cypress": "concurrently 'cross-env BROWSER=none npm start' 'cypress run --browser chrome' --kill-others --success first",
    "test": "npm run test:unit && npm run test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmp/f1cd.git"
  },
  "bugs": {
    "url": "https://github.com/jmp/f1cd/issues"
  },
  "author": "Jarkko Piiroinen",
  "license": "MIT",
  "dependencies": {
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.28",
    "axe-core": "4.6.3",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "cypress": "12.6.0",
    "cypress-axe": "1.3.0",
    "react": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5"
  },
  "overrides": {
    "nth-check": "2.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
